<% content_for :head do %>
  <%= stylesheet_link_tag "shared_index_button", "data-turbo-track": "reload" %>
<% end %>

<%= render "shared/calendar",
    calendar_id: "user-plans-calendar",
    schedules: @schedules_with_roles,
    url_for_event: ->(schedule, role=nil) { 
      users_plan_path(schedule.plans.first) if schedule.plans.any? && role == :plan_participant
    }
%>
